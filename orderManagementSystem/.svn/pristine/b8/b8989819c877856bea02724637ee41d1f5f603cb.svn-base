package com.want.dto;

import java.io.Serializable;
/**
 * SOCS需求订单信息 详情
 * @author 00320558 
 * @date   2019年09月16日
 */
public class OrderDetailDto implements Serializable {
	private static final long serialVersionUID = 5847153539227958372L;
	private String  lineNumber;       //序号
	private String  orderCode;        //订单编码
	private String  productCode;      //订单编码SKU
	private String  prdShortName;     //产品短名称
	private String  productName;      //产品名称（完整）
	private String  salePrice;        //开单价
	private String  discountPrice;    //折后单价
	private String  prodCount;        //数量
	private String  unit;             //单位
	private String  unitName;         //单位名称
	private String  sumAmount;        //合计金额
	private String  itemMemo;         //行备注
	public String getLineNumber() {
		return lineNumber;
	}
	public void setLineNumber(String lineNumber) {
		this.lineNumber = lineNumber;
	}
	public String getOrderCode() {
		return orderCode;
	}
	public void setOrderCode(String orderCode) {
		this.orderCode = orderCode;
	}
	public String getSalePrice() {
		return salePrice;
	}
	public void setSalePrice(String salePrice) {
		this.salePrice = salePrice;
	}
	public String getDiscountPrice() {
		return discountPrice;
	}
	public void setDiscountPrice(String discountPrice) {
		this.discountPrice = discountPrice;
	}
	public String getProdCount() {
		return prodCount;
	}
	public void setProdCount(String prodCount) {
		this.prodCount = prodCount;
	}
	public String getUnit() {
		return unit;
	}
	public void setUnit(String unit) {
		this.unit = unit;
	}
	public String getUnitName() {
		return unitName;
	}
	public void setUnitName(String unitName) {
		this.unitName = unitName;
	}
	public String getSumAmount() {
		return sumAmount;
	}
	public void setSumAmount(String sumAmount) {
		this.sumAmount = sumAmount;
	}
	public String getItemMemo() {
		return itemMemo;
	}
	public void setItemMemo(String itemMemo) {
		this.itemMemo = itemMemo;
	}
	public String getPrdShortName() {
		return prdShortName;
	}
	public void setPrdShortName(String prdShortName) {
		this.prdShortName = prdShortName;
	}
	public String getProductName() {
		return productName;
	}
	public void setProductName(String productName) {
		this.productName = productName;
	}
	public String getProductCode() {
		return productCode;
	}
	public void setProductCode(String productCode) {
		this.productCode = productCode;
	}
	@Override
	public String toString() {
		return "OrderDetailDto [lineNumber=" + lineNumber + ", orderCode=" + orderCode + ", productCode=" + productCode
				+ ", prdShortName=" + prdShortName + ", productName=" + productName + ", salePrice=" + salePrice
				+ ", discountPrice=" + discountPrice + ", prodCount=" + prodCount + ", unit=" + unit + ", unitName="
				+ unitName + ", sumAmount=" + sumAmount + ", itemMemo=" + itemMemo + "]";
	}
}
